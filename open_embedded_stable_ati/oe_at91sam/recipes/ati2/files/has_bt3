echo -n "Trigger 3 Expose: "
/usr/bin/eeprom_rw read -addr 0x688 -size 0x04
echo

echo -n "Trigger 3 On Hit: "
/usr/bin/eeprom_rw read -addr 0x68C -size 0x04
echo

echo -n "Trigger 3 On Kill: "
/usr/bin/eeprom_rw read -addr 0x690 -size 0x04
echo

echo -n "Trigger 3 Shots: "
/usr/bin/eeprom_rw read -addr 0x6B0 -size 0x04
echo

