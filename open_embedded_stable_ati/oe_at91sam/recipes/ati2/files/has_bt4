echo -n "Trigger 4 Expose: "
/usr/bin/eeprom_rw read -addr 0x6C8 -size 0x04
echo

echo -n "Trigger 4 On Hit: "
/usr/bin/eeprom_rw read -addr 0x6CC -size 0x04
echo

echo -n "Trigger 4 On Kill: "
/usr/bin/eeprom_rw read -addr 0x6D0 -size 0x04
echo

echo -n "Trigger 4 Shots: "
/usr/bin/eeprom_rw read -addr 0x6F0 -size 0x04
echo

